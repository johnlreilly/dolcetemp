<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="wyndham-dolce Design"
        sling:resourceType="wcm/core/components/designer">
        <page-todelete jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys"
                components="[group:dolcecomponents]">
                <colctrl
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wyndham-dolce/components/content/colctrl"
                    layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)"/>
                <cq:authoring jcr:primaryType="nt:unstructured">
                    <assetToComponentMapping jcr:primaryType="nt:unstructured">
                        <image
                            jcr:primaryType="nt:unstructured"
                            assetGroup="media"
                            assetMimetype="image/*"
                            droptarget="image"
                            resourceType="wyndham-dolce/components/content/image"
                            type="Images"/>
                    </assetToComponentMapping>
                </cq:authoring>
            </par>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wyndham-dolce/components/structure/logo"
                absParent="2">
                <image/>
            </logo>
        </page-todelete>
        <basepagelayout jcr:primaryType="nt:unstructured"/>
    </jcr:content>
    <clientlib-all/>
    <clientlib-site/>
    <favicon.ico/>
    <css/>
    <js/>
</jcr:root>
